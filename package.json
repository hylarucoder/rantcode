{
  "name": "rantcode",
  "version": "0.0.1",
  "description": "Desktop app for managing AI coding agents like Claude Code and Codex",
  "main": "./out/main/index.js",
  "author": "hylarucoder",
  "homepage": "https://rantcode.hylarucoder.io",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm -s typecheck:node && pnpm -s typecheck:web",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "pnpm -s typecheck && pnpm -s lint",
    "clean": "rimraf dist out .eslintcache",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "pnpm -s typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm -s build && electron-builder --dir",
    "build:win": "pnpm -s build && electron-builder --win",
    "build:mac": "pnpm -s build && electron-builder --mac",
    "build:linux": "pnpm -s build && electron-builder --linux",
    "build:stable": "tsx scripts/build-channel.ts stable",
    "build:nightly": "tsx scripts/build-channel.ts nightly",
    "release:stable": "tsx scripts/build-channel.ts stable --publish",
    "release:nightly": "tsx scripts/build-channel.ts nightly --publish"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@libsql/client": "^0.15.8",
    "@orpc/client": "^1.11.2",
    "@orpc/contract": "^1.11.2",
    "@orpc/server": "^1.11.2",
    "@orpc/tanstack-query": "^1.11.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@shikijs/rehype": "^3.15.0",
    "@tanstack/react-query": "^5.51.21",
    "@theguild/remark-mermaid": "0.3.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "chokidar": "^3.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.44.7",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.3.9",
    "electron-window-state": "^5.0.3",
    "immer": "^10.2.0",
    "lucide-react": "^0.553.0",
    "mermaid": "^11.12.1",
    "motion": "^12.23.24",
    "react-hook-form": "^7.66.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "react-router": "^7.9.6",
    "rehype-mermaid": "^3.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "shiki": "^3.15.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "unified": "^11.0.5",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron/rebuild": "^4.0.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.18.6",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "drizzle-kit": "^0.31.7",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "jsdom": "^24.1.3",
    "orpc": "^0.1.1",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rehype-sanitize": "^6.0.0",
    "rimraf": "^6.1.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.19.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.9",
    "zustand": "^5.0.8"
  },
  "packageManager": "pnpm@10.17.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}
